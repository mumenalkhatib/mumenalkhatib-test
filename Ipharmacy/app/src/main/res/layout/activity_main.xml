<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.ipharmacy.MainActivity">


    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView

            android:id="@+id/TextView1"
            android:layout_width="160dp"
            android:layout_height="40dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="60dp"
            android:layout_marginTop="120dp"
            android:text="Nearest pharmacy"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />



        <TextView

            android:id="@+id/TextView2"
            android:layout_width="160dp"
            android:layout_height="40dp"
            android:layout_marginTop="120dp"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="-15dp"
            android:text="Health insurance"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content">



        <ImageView

            android:id="@+id/imageView1"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="60dp"
            android:layout_marginLeft="40dp"
            app:srcCompat="@drawable/nearest" />


        <ImageView

            android:id="@+id/imageView2"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="40dp"
            app:srcCompat="@drawable/l" />

    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout">

        <TextView

            android:id="@+id/TextView3"
            android:layout_width="160dp"
            android:layout_height="40dp"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="60dp"
            android:layout_marginTop="330dp"
            android:text="Alternations"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

        <TextView

            android:id="@+id/TextView4"
            android:layout_width="160dp"
            android:layout_height="40dp"
            android:layout_marginTop="330dp"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="-40dp"
            android:text="Alternative medicine"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    </LinearLayout>


    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">


        <ImageView

            android:id="@+id/imageView3"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="60dp"
            android:layout_marginTop="220dp"
            app:srcCompat="@drawable/aaa" />

        <ImageView

            android:id="@+id/imageView4"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="220dp"
            app:srcCompat="@drawable/oo" />

    </LinearLayout>

    <ImageView

        android:id="@+id/imageView5"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@+id/linearLayout"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp"
        app:srcCompat="@drawable/k" />





    <TextView

        android:id="@+id/TextView5"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageView5"
        android:text="Feedback"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

</RelativeLayout>

